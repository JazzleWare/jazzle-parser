                                
'ArrayExpression'            yield                                   emitter
'ArrowFunctionExpression'    not-needed                                        
'AssignmentExpression'       yield                                   emitter
'AssignmentPattern'          yield (with AssignmentExpression)       emitter                                   
'AssignmentProperty'         yield (with AssignmentExpression)       emitter                                        
'BinaryExpression'           yield                                   emitter
'CallExpression'             yield                                   emitter 
'ClassBody'                                                          
'ClassExpression'                                                    
'ConditionalExpression'      yield                                   emitter                                              
'FunctionExpression'         not-needed                                         
'Identifier'                 not-needed                              emitter
'Literal'                    not-needed                              emitter
'LogicalExpression'          yield                                   emitter 
'MemberExpression' val       yield                                   emitter          
'MemberExpression' ref       yield                                   emitter (same as val)          
'MetaProperty'                
'MethodDefinition'
'NewExpression' 
'ObjectExpression'
'ArrayPattern'               yield (with AssignmentExpression)       emitter
'ObjectPattern'              yield (with AssignmentExpression)
'Property'                   yield (with ObjectExpression)
'RestElement'
'SequenceExpression' 
'SpreadElement'
'Super'
'TaggedTemplateExpression'
'TemplateElement'
'TemplateLiteral'
'ThisExpression'             not-needed 
'UnaryExpression' 
'UpdateExpression' 
'YieldExpression'
